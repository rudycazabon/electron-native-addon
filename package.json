{
  "name": "electron-native-addon",
  "productName": "electron-native-addon",
  "description": "test packaging of a native addon with electron",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "node-gyp rebuild --arch=x64 --target=8.3.0 --dist-url=https://electronjs.org/headers",
    "package-win": "electron-packager . electron-native-addon --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"electron-native-addon\"",
    "create-installer-win": "node installers/windows/createinstaller.js"
  },
  "author": "Rudy Cazabon",
  "license": "MIT",
  "devDependencies": {
    "asar": "^3.0.3",
    "bindings": "^1.2.1",
    "electron": "^8.3.0",
    "electron-packager": "^14.2.1",
    "electron-rebuild": "^1.11.0",
    "electron-tabs": "^0.15.0",
    "electron-winstaller": "^2.7.0",
    "grunt": "^1.1.0",
    "nan": "^2.14.1",
    "node-addon-api": "^3.0.0",
    "node-gyp": "^6.1.0"
  },
  "dependencies": {
    "node-addon-api": "^3.0.0",
    "bindings": "^1.2.1"
  },
  "gypfile": true
}
